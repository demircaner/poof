{
  "manifest_version": 2,
  "name": "ZoomCloser",
  "version": "1.0",
  "description": "Close those pesky Zoom tabs!",
  "background": {
    "scripts": [
      "src/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/icons8-wind-50.png"
  },
  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["src/content.js"],
          "css": ["src/styles.css"]
      }
  ]
}